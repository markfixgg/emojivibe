{
  "manifest_version": 3,
  "name": "Emoji Keyboard - Emojivibe",
  "description": "The keyboard of emojis online - copy and paste any emoji or emoji combo.",
  "version": "1.0.5",
  "background": {
    "service_worker": "background.bundle.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon-34.png"
  },
  "icons": {
    "128": "icon-128.png"
  },
  "content_scripts": [],
  "permissions": [
    "tabs"
  ]
}
